<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="565.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerCaroBoard">
    <children>

        <Label layoutX="9.0" layoutY="17.0" text="Name player 1" textFill="#b55b27">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="7.0" layoutY="62.0" text="Name player  2" textFill="#a6a82c">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Button fx:id="exit" layoutX="13.0" layoutY="446.0" mnemonicParsing="false" onAction="#CaroBoardExit" prefHeight="30.0" prefWidth="90.0" text="Exit" textFill="#252522">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Label fx:id="lbName1" layoutX="7.0" layoutY="37.0" prefHeight="17.0" prefWidth="107.0" textFill="#cd2525">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label fx:id="lbName2" layoutX="7.0" layoutY="95.0" prefHeight="17.0" prefWidth="107.0" textFill="#f20303">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
      <Button fx:id="NewGame" layoutX="9.0" layoutY="401.0" mnemonicParsing="false" onAction="#NewGame" prefHeight="30.0" prefWidth="90.0" text="New Game" />
      <Button fx:id="start" layoutX="13.0" layoutY="358.0" mnemonicParsing="false" onAction="#StartGame" prefHeight="30.0" prefWidth="90.0" text="Start">
         <font>
            <Font size="14.0" />
