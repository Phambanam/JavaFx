      </Label>
      <Label layoutX="16.0" layoutY="131.0" text="Score">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="lbScore1" alignment="CENTER" layoutX="58.0" layoutY="131.0" textFill="#cd2727">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="70.0" layoutY="129.0" prefHeight="17.0" prefWidth="15.0" text=":" />
      <Label fx:id="lbScore2" layoutX="90.0" layoutY="131.0" textFill="#b2c992">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
