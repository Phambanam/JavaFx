<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Model.Controller">
    <children>
        <Button fx:id="start" layoutX="179.0" layoutY="252.0" mnemonicParsing="false" onAction="#btStart"
                prefHeight="34.0" prefWidth="106.0" style="-fx-background-color: Pink;" text="Start" textFill="#eb0808">
            <font>
                <Font name="Elephant Italic" size="16.0"/>
            </font>
        </Button>
        <TextField fx:id="name1" layoutX="220.0" layoutY="102.0" onAction="#checkName1" prefHeight="25.0"
                   prefWidth="184.0">
            <font>
                <Font name="Elephant Italic" size="16.0"/>
            </font>
        </TextField>
        <Label contentDisplay="CENTER" layoutX="87.0" layoutY="22.0" text="NAME PLAYER" textFill="#7dd358">
            <font>
                <Font name="Elephant Italic" size="40.0"/>
            </font>
        </Label>
        <Label layoutX="93.0" layoutY="107.0" text="Name player1" textFill="#b53c3c">
            <font>
                <Font name="Elephant Italic" size="16.0"/>
            </font>
        </Label>
        <Label layoutX="93.0" layoutY="156.0" text="Name player2" textFill="#2b199e">
            <font>
                <Font name="Elephant Italic" size="16.0"/>
            </font>
        </Label>
        <TextField fx:id="name2" layoutX="221.0" layoutY="151.0" onAction="#checkName2" prefHeight="25.0"
                   prefWidth="184.0">
            <font>
                <Font name="Elephant Italic" size="16.0"/>
            </font>
        </TextField>
        <Button fx:id="btExit" layoutX="416.0" layoutY="252.0" mnemonicParsing="false" onAction="#exitGame"
                prefHeight="31.0" prefWidth="106.0" style="-fx-background-color: pink;" text="Exit">
            <font>
                <Font name="Elephant Italic" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="btTutorial" layoutX="300.0" layoutY="252.0" mnemonicParsing="false" onAction="#btTutorial"
                prefHeight="34.0" prefWidth="106.0" style="-fx-background-color: Pink;" text="Tutorial"
                textFill="#3abf1f">
            <font>
                <Font name="Elephant Italic" size="16.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
